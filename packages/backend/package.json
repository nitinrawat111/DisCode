{
  "name": "@discode/backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && dotenvx run -f .env -- node dist/src/server.js",
    "lint": "eslint . --fix",
    "pretty": "prettier --write .",
    "db:start": "docker compose up -d database",
    "db:migrate:make": "kysely migrate:make ",
    "db:migrate:up": "dotenvx run -f .env -- kysely migrate:up",
    "db:migrate:down": "dotenvx run -f .env -- kysely migrate:down",
    "db:migrate:latest": "dotenvx run -f .env -- kysely migrate:latest",
    "db:rollback": "dotenvx run -f .env -- kysely migrate:rollback",
    "db:seed:run": "dotenvx run -f .env -- kysely seed:run",
    "db:seed:make": "kysely seed:make"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^25.2.3",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "kysely-ctl": "^0.13.1",
    "npm": "^11.2.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.22.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jose": "^6.0.10",
    "kysely": "^0.28.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.1",
    "pg": "^8.13.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  }
